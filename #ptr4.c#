#include<stdlib.h>
#include<stdio.h>
#include<string.h>
#include <sys/types.h>
#include<unistd.h>
#include <stdarg.h>


char*  mstr_chr(char *s,char ch){
  char *p=s;
  while(*p!=ch)p++;
  if(*p=='\0')
    return NULL;
  return p;
}

int  main(int argc,char **argv){
  int a=3,b=4;
  printf("helloworld");
  return 0;
  
}
